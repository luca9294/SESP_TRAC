<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sesp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sesp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sesp.MainActivity2"
            android:label="@string/title_activity_wiki" >
        </activity>
        <activity
            android:name="com.example.sesp.WikiActivity"
            android:label="@string/title_activity_wiki" >
        </activity>
        <activity
            android:name="com.example.sesp.ListWikiActivity"
            android:label="@string/title_activity_list_wiki" >
        </activity>
        <activity
            android:name="com.example.sesp.TicketActivity"
            android:label="@string/title_activity_ticket" >
        </activity>
    </application>

</manifest>
